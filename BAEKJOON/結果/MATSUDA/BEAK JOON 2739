
// BEAK JOON No.2739 九九算

import java.util.Scanner;

public class Study {
	public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);
		int N = scanner.nextInt();
		
		for (int i = 1; i < 9 +1; i++) {
			System.out.println(N + "*" + i + "=" + N*i);
		}
		
	}
}

//入力例
//２

//出力例
//2 * 1 = 2
//2 * 2 = 4
//2 * 3 = 6
//2 * 4 = 8
//2 * 5 = 10
//2 * 6 = 12
//2 * 7 = 14
//2 * 8 = 16
//2 * 9 = 18
